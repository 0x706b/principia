{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "declaration": false,
    "sourceMap": true,
    "module": "esnext",
    "target": "es2020",
    "rootDir": "../src-cjs",
    "outDir": "../dist/dist-traced/cjs",
    "tsBuildInfoFile": "../tsbuildinfo/traced-cjs.tsbuildinfo",
    "plugins": [
      {
        "transform": "../../compile/src/index.ts",
        "moduleMap": {
          "^src/(.*)": "(@principia/ptest): _src/$1"
        },
        "createRequire": false,
        "relativeProjectRoot": "../../"
      }
    ]
  },
  "exclude": ["../build", "../dist", "../node_modules", "../test"],
  "include": ["../src-cjs"]
}
