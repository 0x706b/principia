{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "declaration": false,
    "module": "es2020",
    "target": "es2020",
    "rootDir": "../src-esm",
    "outDir": "../dist/dist-traced/esm-fix",
    "sourceMap": true,
    "plugins": [
      {
        "after": true,
        "transform": "@0x706b/ts-transform-fix-esm",
        "relativeProjectRoot": "../../"
      },
      {
        "transform": "../../compile/src/index.ts",
        "moduleMap": {
          "^src/(.*)": "(@principia/ptest): _src/$1"
        }
      }
    ],
    "tsBuildInfoFile": "../tsbuildinfo/traced-esm-fix.tsbuildinfo"
  },
  "exclude": ["../build", "../dist", "../node_modules", "../test"],
  "include": ["../src-esm"]
}
