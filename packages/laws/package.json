{
  "name": "@principia/laws",
  "version": "0.5.4",
  "description": "Laws",
  "repository": "git://github.com/0x706b/principia",
  "license": "MIT",
  "author": "Peter Krol <peter@peterkrol.com>",
  "exports": {
    ".": {
      "traced": {
        "browser": "./dist/dist-traced/esm/index.js",
        "node": {
          "import": "./dist/dist-traced/node/index.js",
          "require": "./dist/dist-traced/cjs/index.js"
        }
      },
      "node": {
        "import": "./dist/dist/node/index.js",
        "require": "./dist/dist/cjs/index.js"
      },
      "browser": "./dist/dist/esm/index.js"
    },
    "./*": {
      "traced": {
        "browser": "./dist/dist-traced/esm/*",
        "node": {
          "import": "./dist/dist-traced/node/*",
          "require": "./dist/dist-traced/cjs/*"
        }
      },
      "browser": "./dist/dist/esm/*",
      "node": {
        "import": "./dist/dist/node/*",
        "require": "./dist/dist/cjs/*"
      }
    }
  },
  "main": "dist/dist/cjs/index.js",
  "module": "./dist/dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "autofix": "yarn lint --fix",
    "build": "yarn build:types && yarn build:node && yarn build:intermediary && yarn build:esm && yarn build:cjs",
    "build:cjs": "babel --config-file ./config/cjs.babelrc.json --extensions '.js' --source-maps true --out-dir dist/dist/cjs dist/dist/.intermediary-build",
    "build:esm": "babel --config-file ./config/esm.babelrc.json --extensions '.js' --source-maps true --out-dir dist/dist/esm dist/dist/.intermediary-build",
    "build:intermediary": "ttsc --build ./config/tsconfig.intermediary.json",
    "build:node": "ttsc --build ./config/tsconfig.node.json",
    "build:postprocess": "ts-node ../../scripts/rewrite-source-maps.ts && ts-node ../../scripts/package.ts && yarn sort-package-json ./dist/package.json && yarn prettier --write ./dist/package.json",
    "build:test": "ttsc --build ./config/tsconfig.test.json --verbose",
    "build:traced": "yarn build:traced:node && yarn build:traced:intermediary && yarn build:traced:esm && yarn build:traced:cjs",
    "build:traced:cjs": "babel --config-file ./config/esm.babelrc.json --extensions '.js' --source-maps true --out-dir dist/dist-traced/cjs dist/dist/.traced-intermediary-build",
    "build:traced:esm": "babel --config-file ./config/esm.babelrc.json --extensions '.js' --source-maps true --out-dir dist/dist-traced/esm dist/dist/.traced-intermediary-build",
    "build:traced:node": "ttsc --build ./config/tsconfig.traced.node.json",
    "build:traced:intermediary": "ttsc --build ./config/tsconfig.traced.intermediary.json",
    "build:types": "ttsc --build ./config/tsconfig.types.json --verbose",
    "buildAll": "yarn build && yarn build:traced && yarn build:postprocess",
    "clean": "rimraf dist build tsbuildinfo",
    "clean:test": "rimraf build",
    "generateSubpaths": "ts-node ../../scripts/subpath.ts && sort-package-json && prettier --write package.json",
    "lint": "eslint .",
    "prepublish": "yarn buildAll && rimraf ./dist/dist/.intermediary-build && rimraf ./dist/dist/.traced-intermediary-build"
  },
  "dependencies": {
    "@principia/base": "^0.5.4",
    "@principia/compile": "^0.4.4",
    "fast-check": "2.13.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "gitHead": "a13f2db4610deafdc4b17a60907c7338917aa702"
}
