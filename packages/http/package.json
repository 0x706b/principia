{
  "name": "@principia/http",
  "version": "0.3.0",
  "description": "HTTP",
  "repository": "git://github.com/0x706b/principia",
  "license": "MIT",
  "author": "Peter Krol <peter@peterkrol.com>",
  "exports": {
    ".": {
      "import": "./dist/dist/esm-fix/index.js",
      "require": "./dist/dist/cjs/index.js"
    },
    "./*": {
      "import": "./dist/dist/esm-fix/*",
      "require": "./dist/dist/cjs/*.js"
    }
  },
  "main": "dist/_dist_/cjs/index.js",
  "module": "./dist/_dist_/esm-shake/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "autofix": "yarn lint --fix",
    "build": "yarn build:types && yarn build:esm-fix && yarn build:intermediary && yarn build:esm-shake && yarn build:cjs && yarn build:postprocess",
    "build:cjs": "babel --config-file ./config/cjs.babelrc.json --extensions '.js' --source-maps true --out-dir dist/dist/cjs dist/dist/.intermediary",
    "build:esm-fix": "ttsc --build ./config/tsconfig.esm-fix.json",
    "build:esm-shake": "babel --config-file ./config/esm.babelrc.json --extensions '.js' --source-maps true --out-dir dist/dist/esm-shake dist/dist/.intermediary",
    "build:intermediary": "ttsc --build ./config/tsconfig.intermediary.json",
    "build:postprocess": "ts-node ../../scripts/rewrite-source-maps.ts && ts-node ../../scripts/package.ts && rimraf dist/dist/.intermediary && yarn sort-package-json ./dist/package.json",
    "build:test": "ttsc --build ./config/tsconfig.test.json --verbose",
    "build:types": "ttsc --build ./config/tsconfig.types.json --verbose",
    "clean": "rimraf dist build tsbuildinfo",
    "clean:test": "rimraf build",
    "generateSubpaths": "ts-node ../../scripts/subpath.ts && sort-package-json && prettier --write package.json",
    "lint": "eslint .",
    "prepublish": "yarn clean && yarn build",
    "publish:package": "yarn version --no-git-tag-version && yarn clean && yarn build && yarn publish --no-git-tag-version dist",
    "test": "yarn build:test && jest build/test"
  },
  "dependencies": {
    "@principia/base": "^0.3.0",
    "@principia/compile": "^0.3.0",
    "@principia/free": "^0.3.0",
    "@principia/io": "^0.3.0",
    "@principia/model": "^0.3.0",
    "@principia/node": "^0.3.0",
    "@types/qs": "^6.9.5",
    "core-js": "3",
    "path-to-regexp": "^6.2.0",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.9"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "gitHead": "185e8063ab953917c77482a33f8885fdd554ddf3"
}
