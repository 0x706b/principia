{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "declaration": false,
    "module": "es6",
    "target": "es2020",
    "rootDir": "../src",
    "outDir": "../dist/dist-traced/node",
    "sourceMap": true,
    "plugins": [
      {
        "transform": "../../compile/src/index.ts",
        "moduleMap": {
          "^src/(.*)": "(@principia/http): _src/$1"
        },
        "relativeProjectRoot": "../../"
      }
    ],
    "tsBuildInfoFile": "../tsbuildinfo/traced-node.tsbuildinfo"
  },
  "exclude": ["../build", "../dist", "../node_modules", "../test"],
  "include": ["../src"]
}
