{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "declaration": false,
    "sourceMap": true,
    "module": "es2015",
    "target": "es2020",
    "lib": ["es2020"],
    "rootDir": "../src-cjs",
    "outDir": "../dist/dist/.intermediary",
    "tsBuildInfoFile": "../tsbuildinfo/esm.tsbuildinfo",
    "plugins": [
      {
        "transform": "../../compile/src/index.ts"
      }
    ]
  },
  "exclude": ["../build", "../dist", "../node_modules", "../test"],
  "include": ["../src-cjs"]
}
